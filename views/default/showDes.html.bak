<link rel="stylesheet" href="{{=URL('static','css/bootstrap.min.css')}}"/>
<h1 style="text-align:center">{{=des.eventName}}</h1>
{{=des.description}}
<ul class="table table-bordered table-striped">
    {{=LI('Starts At:   ', des.startAt)}}
    {{=LI('Ends At:   ', des.endAt)}}
    {{=LI('Venue:   ', des.venue)}}
</ul>

{{if des.contact or des.link:}}
{{=H1('Contact information')}}
{{=LI('Phone number:   ', des.contact)}}
{{=LI('Link: ', des.link)}}
{{pass}}
